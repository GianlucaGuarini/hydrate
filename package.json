{
  "name": "@riotjs/hydrate",
  "version": "4.0.1",
  "description": "Riot.js hydrate strategy for SSR applications",
  "main": "hydrate.js",
  "jsnext:main": "src/index.js",
  "module": "src/index.js",
  "scripts": {
    "prepare": "npm i riot@5 --no-save",
    "prepublishOnly": "npm run build && npm test",
    "lint": "eslint src/*.js test/*.js rollup.config.js",
    "build": "rollup -c",
    "pretest": "npm run build",
    "test": "npm run lint && mocha -r esm --exit test/*.js"
  },
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GianlucaGuarini/@riotjs/hydrate.git"
  },
  "keywords": [
    "riot",
    "hydrate",
    "ssr",
    "Riot.js",
    "components"
  ],
  "peerDependencies": {
    "riot": "^5.0.0"
  },
  "author": "Gianluca Guarini <gianluca.guarini@gmail.com> (http://gianlucaguarini.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GianlucaGuarini/@riotjs/hydrate/issues"
  },
  "homepage": "https://github.com/GianlucaGuarini/@riotjs/hydrate#readme",
  "devDependencies": {
    "@riotjs/compiler": "^5.0.0",
    "@riotjs/ssr": "^5.0.0",
    "chai": "^4.2.0",
    "eslint": "^7.12.1",
    "eslint-config-riot": "^3.0.0",
    "esm": "^3.2.25",
    "jsdom": "16.4.0",
    "jsdom-global": "3.0.2",
    "mocha": "^8.2.1",
    "rollup": "^2.33.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0"
  },
  "dependencies": {
    "morphdom": "^2.6.1"
  }
